local player = game.Players.LocalPlayer
local mouse = player:GetMouse()
local amongus = game.Workspace:WaitForChild("amongus")
local maxwell = game.Workspace:WaitForChild("Cat")
local model = game.Workspace:WaitForChild("Dog")

while true do
	wait()
	
	local mousetarget = mouse.Target
	if mousetarget then
		mousetarget:Destroy()	
	end

	
